{
  "name": "@chrisaugu/kasap",
  "productName": "Kasap",
  "description": "Kasap is a ticket processing software built on ElectronJs",
  "keywords": [],
  "version": "1.0.0",
  "author": "Christian Augustyn",
  "private": true,
  "xmain": "./.webpack/main",
  "main": "./src/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "electron-start": "electron .",
    
    "dev": "concurrently -k \"set BROWSER=none && yarn react-start\" \"yarn:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "package-react": "yarn build && yarn package",
    "make-mac": "yarn build && yarn make --platform darwin",
    "make-linux": "yarn build && yarn make --platform linux",
    "make-react": "yarn build && yarn make",

    "forge-start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "license": "MIT",
  "repository": {
    "url": "git@github.com:chrisaugu/kasap.git"
  },
  "publishConfig": {
    "@chrisaugu:registry": "https://npm.pkg.github.com"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
